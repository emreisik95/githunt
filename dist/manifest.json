{
  "manifest_version": 3,
  "name": "GitHunt",
  "short_name": "GitHunt",
  "description": "Replace the new tab with a list of trending repositories on GitHub.",
  "version": "4.0.0",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://api.github.com/*"
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_title": "GitHunt",
    "default_icon": "img/icon-128.png"
  }
}
